{
  "name": "project-starter",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=22.2.0",
    "npm": ">=10.7.0"
  },
  "scripts": {
    "dev": "run-p dev:postcss dev:browser-sync",
    "build": "run-s lint build:reset build:imagemin build:prettier build:browser-sync",
    "lint": "run-s lint:postcss lint:markuplint lint:eslint lint:stylelint",
    "dev:postcss": "postcss 'src/assets/_postcss/main.css' -o 'src/assets/styles/main.css' --watch",
    "dev:browser-sync": "browser-sync start --server 'src' --watch 'src/**/*'",
    "build:reset": "rimraf 'dist' && cpx 'src/**/*' 'dist'",
    "build:imagemin": "node 'imagemin.mjs' --initial",
    "build:prettier": "prettier 'dist' --write",
    "build:browser-sync": "browser-sync start --server 'dist' --watch 'dist/**/*'",
    "lint:postcss": "postcss 'src/assets/_postcss/main.css' -o 'src/assets/styles/main.css'",
    "lint:markuplint": "markuplint 'src/**/*.html' && figlet 'HTML Lint completed.'",
    "lint:eslint": "eslint 'src/**/*.js' --fix && figlet 'JavaScript Lint completed'",
    "lint:stylelint": "stylelint 'src/**/*.css' --fix && figlet 'CSS Lint completed'"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@iconify/json": "^2.2.339",
    "@iconify/tailwind4": "^1.0.6",
    "@obukata/tailwind-plugin-utility": "^1.0.0",
    "@obukata/tailwind-typography-ckeditor": "^1.2.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.7",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "cpx": "^1.5.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.18.0",
    "eslint-plugin-promise": "^7.2.1",
    "figlet-cli": "^0.3.0",
    "gsap": "^3.13.0",
    "imagemin-cli": "^8.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "markuplint": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "postcss-import": "^16.1.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-mixins": "^11.0.3",
    "postcss-preset-env": "^10.1.6",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.4.2",
    "prettier-eslint-cli": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4.1.7"
  }
}
